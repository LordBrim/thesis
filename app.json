{
  "expo": {
    "experiments": {
      "typedRoutes": true
    },
    "scheme": "acme",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "name": "Lifeline",
    "slug": "lifeline",
    "icon": "./assets/splash/icon.png",
    "extra": {
      "router": {
        "origin": false
      }
    },
    "splash": {
      "image": "./assets/splash/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF3642"
    },
    "android": {
      "package": "com.andreisager.lifeline",
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash/adaptive-icon.png",
        "backgroundColor": "#FF3642"
      }
    }
  }
}
