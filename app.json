{
  "expo": {
    "experiments": {
      "typedRoutes": true
    },
    "scheme": "acme",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/iconFinal.png",
          "color": "#FF3642",
          "sounds": ["./assets/notifSound.wav"],
          "androidMode": "default",
          "androidCollapsedTitle": "Lifeline Notification"
        }
      ]
    ],
    "name": "Lifeline",
    "slug": "lifeline",
    "icon": "./assets/splash/icon.png",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bc31c5ed-1121-46d0-8cc8-bbdf1f6e6b50"
      }
    },
    "splash": {
      "image": "./assets/splash/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF3642"
    },
    "android": {
      "useNextNotificationApi": true,
      "googleServicesFile": "./google-services.json",
      "package": "com.lifeline",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCG2UV8weM9nFuWYQNkEbG9f8ITMiCRk"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash/adaptive-icon.png",
        "backgroundColor": "#FF3642"
      },
      "notification": {
        "icon": "./assets/images/icon.png",
        "color": "#FF3642",
        "androidMode": "default",
        "androidCollapsedTitle": "Lifeline Notification",
        "sound": "./assets/notifSound.wav"
      }
    },
    "owner": "lifeline"
  }
}
