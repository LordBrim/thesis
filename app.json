{
  "expo": {
    "experiments": {
      "typedRoutes": true
    },
    "scheme": "acme",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/montserrat/Montserrat_100Thin.ttf",
            "node_modules/@expo-google-fonts/montserrat/Montserrat_100Thin_Italic.ttf",
            "node_modules/@expo-google-fonts/montserrat/Montserrat_200ExtraLight.ttf",
            "node_modules/@expo-google-fonts/montserrat/Montserrat_200ExtraLight_Italic.ttf",
            "node_modules/@expo-google-fonts/montserrat/Montserrat_300Light.ttf",
            "node_modules/@expo-google-fonts/montserrat/Montserrat_300Light_Italic.ttf",
            "node_modules/@expo-google-fonts/montserrat/Montserrat_400Regular.ttf",
            "node_modules/@expo-google-fonts/montserrat/Montserrat_400Regular_Italic.ttf",
            "node_modules/@expo-google-fonts/montserrat/Montserrat_500Medium.ttf",
            "node_modules/@expo-google-fonts/montserrat/Montserrat_500Medium_Italic.ttf",
            "node_modules/@expo-google-fonts/montserrat/Montserrat_600SemiBold.ttf",
            "node_modules/@expo-google-fonts/montserrat/Montserrat_600SemiBold_Italic.ttf",
            "node_modules/@expo-google-fonts/montserrat/Montserrat_700Bold.ttf",
            "node_modules/@expo-google-fonts/montserrat/Montserrat_700Bold_Italic.ttf",
            "node_modules/@expo-google-fonts/montserrat/Montserrat_800ExtraBold.ttf",
            "node_modules/@expo-google-fonts/montserrat/Montserrat_800ExtraBold_Italic.ttf",
            "node_modules/@expo-google-fonts/montserrat/Montserrat_900Black.ttf",
            "node_modules/@expo-google-fonts/montserrat/Montserrat_900Black_Italic.ttf"
          ]
        }
      ]
    ],
    "name": "Lifeline",
    "slug": "lifeline",
    "icon": "./assets/splash/icon.png",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bc31c5ed-1121-46d0-8cc8-bbdf1f6e6b50"
      }
    },
    "splash": {
      "image": "./assets/splash/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.lifeline",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCG2UV8weM9nFuWYQNkEbG9f8wwITMiCRk"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash/adaptive-icon.png",
        "backgroundColor": "#FF3642"
      }
    },
    "owner": "lifeline",
    "orientation": "portrait"
  }
}
